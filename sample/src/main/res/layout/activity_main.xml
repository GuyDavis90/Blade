<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="@dimen/activity_vertical_margin"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_go_arg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/goto_arg"/>

    <Button
        android:id="@+id/btn_go_extra"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/goto_extra"/>

    <Button
        android:id="@+id/btn_go_parcel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/goto_parcel"/>

    <Button
        android:id="@+id/btn_go_state"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/goto_state"/>

    <Button
        android:id="@+id/btn_go_mvp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/goto_mvp"/>

    <Button
        android:id="@+id/btn_go_kotlin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/goto_kotlin"/>

</LinearLayout>
